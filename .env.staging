# 应用配置
NODE_ENV=staging
PORT=3002
API_URL=http://staging-api.taskshub.com/api

# 数据库配置
DB_HOST=staging-db.taskshub.com
DB_PORT=3306
DB_NAME=taskshub_staging
DB_USER=taskshub_staging_user
DB_PASSWORD=staging_password

# Redis配置
REDIS_HOST=staging-redis.taskshub.com
REDIS_PORT=6379
REDIS_PASSWORD=staging_password

# JWT配置
JWT_SECRET=staging_jwt_secret
JWT_EXPIRES_IN=24h

# 日志配置
LOG_LEVEL=info
LOG_FILE=logs/staging.log

# 监控配置
ENABLE_MONITORING=true
MONITORING_PORT=9092 